<configuration>
<system.webServer>
<handlers>
  <add name="iisnode" path="src/app.js" verb="*" modules="iisnode" />
</handlers>
        <iisnode loggingEnabled="true" devErrorsEnabled="true" />
        <rewrite>
            <rules>
                <rule name="ReverseProxyInboundRule1" stopProcessing="true">
                    <match url="(.*)" />
                    <action type="Rewrite" url="src/app.js" />
                </rule>
            </rules>
        </rewrite> 
</system.webServer>
 </configuration>
