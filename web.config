<configuration>
<system.webServer>
<handlers>
   <remove name="WebDAV" />
  <add name="iisnode" path="src/app.js" verb="*" modules="iisnode" />
  <add name="WebDAV" path="*" verb="GET,POST,PUT,DELETE" modules="WebDAVModule" resourceType="Unspecified" requireAccess="None" />
</handlers>
<rewrite>
  <rules>
    <rule name="nodejs">
      <match url="(.*)" />
      <action type="Rewrite" url="src/app.js" />
    </rule>
  </rules>
</rewrite> 
        <httpProtocol>
            <customHeaders>
                <remove name="X-Powered-By" />
            </customHeaders>
        </httpProtocol>
		
  <modules runAllManagedModulesForAllRequests="false">
    <remove name="WebDAVModule" />
  </modules>
  
    <httpErrors existingResponse="PassThrough" />
</system.webServer>
 </configuration>
